{
  "name": "kaituo/tophp",
  "description": "tophp",
  "type": "project",
  "keywords": [],
  "homepage": "http://thinkphp.cn/",
  "license": "Apache-2.0",
  "authors": [
    {
      "name": "sleep",
      "email": "sleep@kaituocn.com"
    }
  ],
  "require": {
    "php": ">=7.1.0",
    "topthink/framework": "^6.0.0",
    "topthink/think-orm": "^2.0",
    "topthink/think-swoole": "^3.0",
    "topthink/think-annotation": "^1.0",
    "topphp/component-builder": "dev-master"
  },
  "require-dev": {
    "symfony/var-dumper": "^4.2",
    "topthink/think-trace": "^1.0"
  },
  "autoload": {
    "psr-4": {
      "app\\": "app"
    },
    "psr-0": {
      "": "extend/"
    }
  },
  "config": {
    "preferred-install": "dist"
  },
  "scripts": {
    "post-autoload-dump": [
      "@php think service:discover",
      "@php think vendor:publish"
    ]
  },
  "repositories": {
    "skeleton": {
      "type": "path",
      "url": "../component-builder"
    }
  }
}
